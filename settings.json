{
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": false,
  "editor.fontSize": 15,
  "editor.rulers": [],
  "editor.fontFamily": "\"Ligalex Mono\", Ligconsolata, Monaco, 'Courier New', monospace",
  "editor.tabCompletion": "onlySnippets",
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.editor.showTabs": false,
  "editor.minimap.enabled": false,
  "editor.lineHeight": 60,
  "editor.lineNumbers": "off",
  "editor.glyphMargin": false,
  "scm.diffDecorationsGutterWidth": 1,
  "editor.renderIndentGuides": false,
  "vim.insertModeKeyBindings": [
    {
      "before": ["0", "-"],
      "after": ["-", ">"]
    },
    {
      "before": ["-", "="],
      "after": ["=", ">"]
    }
  ],
  "editor.folding": false,
  "phpcs.executablePath": "/Users/jaggy/.composer/vendor/bin/phpcs",
  "phpcs.standard": "PSR2",
  "phpcs.ignorePatterns": ["*/tests/*", "*/database/*"],
  "editor.fontLigatures": true,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "vetur.validation.template": false,
  "emmet.includeLanguages": {
    "HTML (Eex)": "html",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "vue": "html",
    "postcss": "css"
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "[blade]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "workbench.fontAliasing": "auto",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.45,
  "terminal.integrated.letterSpacing": -1,
  "editor.letterSpacing": -1.25,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "elixirLS.suggestSpecs": false
}
